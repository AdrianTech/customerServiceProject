{
  "name": "cls",
  "version": "1.0.0",
  "description": "Client management system",
  "main": "app.js",
  "scripts": {
    "client": "npm run serve --prefix client",
    "server": "nodemon server/app.ts --ignore client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "build": "tsc -b"
  },
  "author": "Adriantech",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.9.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/helmet": "0.0.45",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.7.1",
    "concurrently": "^5.1.0"
  }
