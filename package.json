{
  "name": "cls",
  "version": "1.0.0",
  "description": "Client management system",
  "main": "app.ts",
  "scripts": {
    "client": "npm run serve --prefix client",
    "server": "nodemon server/app.ts --ignore client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node app.js",
    "build": "webpack",
    "build:dev": "webpack && cd client && npm run build",
    "test": "jest --watchAll",
    "start:dev": "SET NODE_ENV=development & nodemon build/app.js"
  },
  "author": "Adriantech",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.uniqby": "^4.7.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.9.1",
    "nodemailer": "^6.4.16"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.157",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongoose": "^5.7.1",
    "@types/node": "^13.7.1",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.10",
    "concurrently": "^5.1.0",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
